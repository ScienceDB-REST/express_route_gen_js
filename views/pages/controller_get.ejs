// get all <%- namePl %>
router.get('/<%- namePlLc %>', function(req, res) {
    models.
    <%- name %>.findAll(helper.paginate(req)).then(function(<%- namePl %>) {
        res.json(<%- namePl %>);
    });
});

// get single <%- name %>
router.get('/<%- nameLc %>/:id', function(req, res) {
    models.
    <%- name %>.find({
        where: {
            id: req.params.id
        }
    }).then(function(x) {
        res.json(x);
    });
});
