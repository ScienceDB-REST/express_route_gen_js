//
// DELETE REQUESTS
//
// delete a <%- nameLc %> or <%- namePl %>
router.delete('/<%- nameLc -%>/(:arr)*', <% include ../partials/acl_middleware %> function(req, res) {

    var params = [req.params.arr].concat(req.params[0].split('/').slice(1));

    models.<%- name -%>.destroy({
        where: {
            id: params
        }
    }).then(function(<%- nameLc -%>) {
        res.json(<%- nameLc -%>);
    }).catch(function(err) {
        res.status(500).json(err)
    })
});
