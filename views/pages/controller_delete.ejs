//
// DELETE REQUESTS
//
// delete a single <%- nameLc %>
router.delete('/<%- nameLc -%>/:id', <% include ../partials/acl_middleware %> function(req, res) {
    models.<%- name -%>.destroy({
        where: {
            id: req.params.id
        }
    }).then(function(<%- nameLc -%>) {
        res.json(<%- nameLc -%>);
    });
});
