//
// PUT REQUESTS
//
// update single <%- nameLc %>
router.put('/<%- nameLc -%>/:id', <% include ../partials/acl_middleware %> function(req, res) {
    models.<%- name -%>.find({
        where: {
            id: req.params.id
        }
    }).then(function(<%- nameLc -%>) {
        if (<%- nameLc -%>) {
            <%- nameLc -%>.updateAttributes({
              <% include ../partials/set_attributes %>
            }).then(function(<%- nameLc -%>) {
                res.send(<%- nameLc -%>);
            });
        }
    });
});
